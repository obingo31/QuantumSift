{
  "name": "quantumsift-smart-contract-security-platform",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Comprehensive smart contract security analysis and vulnerability detection platform",
  "author": "obingo31",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/obingo31/QuantumSift.git"
  },
  "homepage": "https://github.com/obingo31/QuantumSift#readme",
  "bugs": {
    "url": "https://github.com/obingo31/QuantumSift/issues"
  },
  "keywords": [
    "blockchain",
    "smart-contract",
    "security",
    "vulnerability-detection",
    "ethereum",
    "web3"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/react": "^7.54.0",
    "axios": "^1.4.0",
    "ethers": "^5.7.2",
    "framer-motion": "^10.12.16",
    "lucide-react": "^0.244.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "sonner": "^0.4.0",
    "web3": "^1.10.0",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^0.32.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
